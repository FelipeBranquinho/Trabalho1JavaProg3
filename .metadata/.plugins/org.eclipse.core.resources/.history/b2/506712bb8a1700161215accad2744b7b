package jrfp;
import java.util.*;

public class ComparatorInts implements Comparator<Integer>{
	public Map<Integer,Object> base;
	
	public ComparatorInts(Map<Integer,Object> base){
		this.base = base;
	}
	
	@Override
	public int compare(Integer base1, Integer base2){
		return ((Integer)base.get(base1)).compareTo((Integer)base.get(base2));
	}
}
